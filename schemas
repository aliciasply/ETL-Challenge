DROP TABLE IF EXISTS realreal;
DROP TABLE IF EXISTS crownandcaliber;
DROP TABLE IF EXISTS watchbox;

-- Create new table
CREATE TABLE realreal (
	id VARCHAR,
	title VARCHAR,
	model VARCHAR,
	price DEC,
	link VARCHAR
);

CREATE TABLE crownandcaliber (
	id VARCHAR,
	title VARCHAR,
	model VARCHAR,
	price DEC,
	link VARCHAR
);

CREATE TABLE watchbox (
	id VARCHAR,
	title VARCHAR,
	model VARCHAR,
	price DEC,
	link VARCHAR
);
